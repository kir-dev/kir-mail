{
  "name": "mailgun-consumer",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/kir-mail/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "apps/mailgun-consumer/jest.config.ts"
      }
    },
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/mailgun-consumer",
        "main": "apps/mailgun-consumer/src/main.ts",
        "tsConfig": "apps/mailgun-consumer/tsconfig.json",
        "webpackConfig": "apps/mailgun-consumer/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "mailgun-consumer:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "mailgun-consumer:build:development"
        },
        "production": {
          "buildTarget": "mailgun-consumer:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "docker-build": {
      "dependsOn": ["build"],
      "command": "docker build -f apps/mailgun-consumer/Dockerfile . -t kir-mail-mailgun-consumer"
    }
  }
}
